[alias]
    cm = commit --branch --status --verbose
    st = status
    pu = push
    pdo = push -d origin
    pd = push -d
    pl = pull
    co = checkout
    mma = merge master
    ba = branch -a
    bd = branch -d
    bdd = branch -D
    cb = checkout -b
[push]
    default = simple
[core]
    #autocrlf = false
    editor = nvim
[fetch]
    prune = true
[merge "ours"]
	driver = true
